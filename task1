ANS 1.

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

char *addone(char *s,int k){
    for(int i=k-1;i>=0;i--){
        if(s[i]=='0'){
            s[i]='1';
            break;
        }
        else{
            s[i]='0';
        }
    }
    return s;
}

char *subtractone(char *s ,int k){
    for(int i=k-1;i>=0;i--){
        if(s[i]=='0'){
            s[i]='1';
        }
        else{
            s[i]='0';
            break;
        }
    }
    return s;
}
int main(){
int sum=0,n,i;
scanf("%d",&n);
char str[n];
scanf("%s",str);

for( i=0;str[i]!='\0';i++){
    if(str[i]!='0' && str[i]!='1' ){
        printf("-1");
     return 0;}
}

    for( i=1;str[i]=='0' ;i++);
    if(i==n){
        printf("-1");
        return 0;
    }

     for( i=0;str[i]=='1' ;i++);
    if(i==n){
        printf("-1");
        return 0;
    }

   char str1[n];
   strcpy(str1,str);
    printf("%s",subtractone(str,n));
    printf(" ");
    printf("%s",addone(str1,n));

return 0;
}

ANS .2:-

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   int n,found,count=0;
 scanf("%d",&n);
 char str[n+1];
scanf("%s",str);
if(n%2==0){
 while(n!=1){
for(int i=0;i<n/2;i++){
     found=1;
    if(str[i]!=str[n/2+i]){
        found=0;
       break;
    }

}
if(found==1){
        count++;
   n=n/2;
   }
else{
    break;
}
}
printf("%d",count);
}
else{
    printf("0");
}
      
    return 0;
}

ANS.3:-

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n,r,x,y;
scanf("%d%d%d%d",&n,&r,&x,&y);
 int a[n];
  int b[n];
  int k=r;
  for(int i=0;i<n;i++){
    scanf("%d",&a[i]);
  }
  for(int i=0;i<n;i++){
    scanf("%d",&b[i]);
  }
  for(int i=0;i<n;i++){
    if(a[i]==1 && b[i]==1){
        k+=x;
    }
    else if(a[i]==1 && b[i]==0){
        k-=y;
  }
  else{
    k=k;
  }
  }
  if(k>r){
    printf("promoted");}
  else if(k<r){
    printf("demoted");}
  else{
    printf("no change");}
        
    return 0;
}


Ans .4:-
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int t;
scanf("%d",&t);
int n[t];

for(int i=0;i<t;i++){
    scanf("%d",&n[i]);
}
for(int i=0;i<t;i++){
    for(int j=1;j<=n[i]/2+1;j++){
        for(int k=1;k<=n[i];k++){
         if(k>n[i]/2+1-j+1 && k<n[i]/2+j)
            printf(" ");
         else{
            printf("*");
         }
        }
        printf("\n");

    }
    for(int j=n[i]/2+2;j<=n[i];j++){
        for(int k=1;k<=n[i];k++){
            if(k>j-n[i]/2 && k<n[i]-j+n[i]/2+1)
                printf(" ");
            else{
                printf("*");
            }
        }
        printf("\n");
    }
} 
    return 0;
}



